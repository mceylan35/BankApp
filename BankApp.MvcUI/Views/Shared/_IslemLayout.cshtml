<script>$(".form-control.sayi").keyup(function (){
  if (this.value.match(/[^0-9]/g)){
    this.value = this.value.replace(/[^0-9]/g,''); }});</script>
<script>$(".form-control.isim").keyup(function (){
        if (this.value.match(/[^a-zA-Z]/g)){
            this.value = this.value.replace(/[^a-zA-Z]/g,''); }});</script>

<script>$(".yeniHesap").click(function () {
        var uid = $(this).attr("id");

        $.ajax(
            {
                type: "POST",
                url: "/hesap/Create",
                data: { hesapNo: uid },
                success: function (gelenveri) {

                    alert("Hesap Oluşturuldu.")
                    location.reload();
                },
                error: function () {
                    alert("Hesap oluşturulamadı!");
                }
            });
    });</script>
<script>$(".hesapsil").click(function () {
        var eknumarasi = $(this).attr("id");
        $.ajax(
            {
                type: "POST",
                url: "/hesap/sil",
                data: { ekNo: eknumarasi },
                success: function (gelenveri) {

                    alert("Hesap silindi!");
                    location.reload();
                },
                error: function () {
                    alert("Hesap silinemedi!");
                }
            });
    });</script>